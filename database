#script file

#creates user table

CREATE TABLE User(
    user_id INT PRIMARY KEY,
    name VARCHAR(100)
                 );

#creates user table
CREATE TABLE Contributor(
    contributor_id INT PRIMARY KEY,
    name VARCHAR(100)
                        );

CREATE TABLE Song(
    song_id INT PRIMARY KEY,
    main_artist VARCHAR(100) NOT NULL,
    type VARCHAR(50),
    title VARCHAR(150)
                );
